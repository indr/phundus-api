@model DiscriminatorViewModel



    <tr id="@Html.IdFor(m => m)">
        <td>
        @Html.DisplayFor(m => m.Caption)
    
        @Html.HiddenFor(m => m.PropertyDefinitionId)
        @Html.HiddenFor(m => m.PropertyValueId)
        @Html.HiddenFor(m => m.IsDeleted)
        </td>
        <td>
            <a href="#" class="btn btn-danger" onclick="ae.removeDisc('@Html.IdFor(m => m)', '@Model.Caption', @Model.PropertyDefinitionId); return false;">Entfernen</a>
        </td>
       </tr> 

