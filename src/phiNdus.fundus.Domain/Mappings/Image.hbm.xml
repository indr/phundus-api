<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="phiNdus.fundus.Core.Domain" namespace="phiNdus.fundus.Core.Domain.Entities">

  <class name="Image" table="[Image]">
    <id name="Id">
      <generator class="hilo" />
    </id>
    
    <version name="Version" />
    <many-to-one name="Article" class="Article" column="ArticleId" />
    <property name="IsPreview" />
    <property name="Length" />
    <property name="Type" />
    <property name="FileName" />
  </class>
</hibernate-mapping>