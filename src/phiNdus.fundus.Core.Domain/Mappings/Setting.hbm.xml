<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="phiNdus.fundus.Core.Domain"
                   namespace="phiNdus.fundus.Core.Domain.Entities">

  <class name="Setting" table="[Setting]">
    <id name="Id">
      <generator class="hilo" />
    </id>
    <version name="Version" />
    
    <property name="Key" column="[Key]" update="false" />
    <property name="StringValue" access="field.camelcase-underscore" />
    <property name="IntegerValue" access="field.camelcase-underscore" />
    <property name="DecimalValue" access="field.camelcase-underscore" />
  </class>
</hibernate-mapping>