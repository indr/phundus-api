<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Phundus.Core" namespace="Phundus.Core.Shop.Orders.Model">
  <class name="CartItem" table="[CartItem]" schema-action="validate">
    <id name="Id">
      <generator class="hilo" />
    </id>
    <version name="Version" />

    <many-to-one name="Cart" class="Cart" column="CartId" />

    <property name="Quantity" />
    <property name="From" column="[From]" />
    <property name="To" column="[To]" />

    <many-to-one name="Article" class="Phundus.Core.Inventory.Articles.Model.Article" column="ArticleId" />
  </class>
</hibernate-mapping>