@model phiNdus.fundus.Web.ViewModels.ShopArticleViewModel

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Datum</th>
            <th>Menge</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var each in Model.Availabilities)
        {
            <tr>
                <td>@Html.DisplayFor(m => each.Date)</td>
                <td>@Html.DisplayFor(m => each.Amount)</td>
            </tr>
        }
    </tbody>
</table>