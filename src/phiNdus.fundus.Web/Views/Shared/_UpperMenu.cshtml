@inherits System.Web.Mvc.WebViewPage<dynamic>
              
<p class="navbar-text pull-right">
@if (Request.IsAuthenticated)
{   
    @Html.ActionLink("Warenkorb", "Index", "Cart", null, new { id = "cart" }) @:|

    @Html.Raw("Angemeldet als ")@Html.ActionLink(User.Identity.Name, "Profile", "Account") @:(@Html.ActionLink("Abmelden", "LogOff", "Account"))
    

        
}
else
{
    @Html.ActionLink("Registrieren", "SignUp", "Account") @:|
    @Html.ActionLink("Anmelden", "LogOn", "Account")
}
</p>