<?xml version="1.0" encoding="utf-8"?>
<parameters>
  <parameter name="Server URL">
    <parameterEntry kind="XmlFile"
                    scope="\\Web\.config$"
                    match="/configuration/appSettings/add[@key='ServerUrl']/@value" />
  </parameter>
  
  <parameter name="Feedback Recipients">
    <parameterEntry kind="XmlFile"
                    scope="\\Web\.config$"
                    match="/configuration/appSettings/add[@key='FeedbackRecipients']/@value" />
  </parameter>

  <parameter name="SMTP From">
    <parameterEntry kind="XmlFile"
                    scope="\\Web\.config$"
                    match="/configuration/system.net/mailSettings/smtp/@from" />
  </parameter>

  <parameter name="Migration Tags"
             defaultValue="">
    <parameterEntry kind="XmlFile"
                    scope="\\Web\.config$"
                    match="/configuration/appSettings/add[@key='MigrationTags']/@value" />
  </parameter>

  <parameter name="Migration Profile"
             defaultValue="">
    <parameterEntry kind="XmlFile"
                    scope="\\Web\.config$"
                    match="/configuration/appSettings/add[@key='MigrationProfile']/@value" />
  </parameter>
</parameters>